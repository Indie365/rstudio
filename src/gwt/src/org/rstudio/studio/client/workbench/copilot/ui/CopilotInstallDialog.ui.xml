<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:rw='urn:import:org.rstudio.core.client.widget'
             ui:generateFormat='com.google.gwt.i18n.server.PropertyCatalogFactory'
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateLocales="default">
              
	<ui:style>
	
	.container {
		margin-left: 6px;
	}
	
	.container p {
		margin-top: 0;
		margin-bottom: 16px;
	}
	
	</ui:style>
	
	<div class="{style.container}">
		
		<p ui:field="alreadyInstalledMessage_"></p>
		
		<p><ui:msg key="copilotTerms">
			Your use of GitHub Copilot is governed by the GitHub Copilot terms of service.
			By using this software, you agree to abide by the GitHub Copilot terms of service.
		</ui:msg></p>
		
	</div>
	
</ui:UiBinder>