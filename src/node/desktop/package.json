{
  "name": "rstudio",
  "productName": "RStudio",
  "version": "99.9.9",
  "description": "RStudio",
  "main": "./.webpack/main",
  "repository": "https://github.com/rstudio/rstudio",
  "author": "RStudio PBC",
  "license": "AGPL-3.0-only",
  "scripts": {
    "clean": "ts-node scripts/clean.ts",
    "lint": "eslint ./src ./test",
    "package": "npm ci && electron-forge package",
    "start": "electron-forge start",
    "debug": "electron-forge start --inspect-electron",
    "fullstart": "npm install && electron-forge start",
    "test": "electron-mocha --no-sandbox --full-trace --config ./test/unit/mocharc.json",
    "testcover": "nyc electron-mocha -c --config ./test/unit/mocharc.json",
    "testwip": "electron-mocha -c -g WIP --config ./test/unit/mocharc.json",
    "format": "npx prettier --write . --ignore-path .gitignore",
    "postinstall": "npx electron-rebuild && ts-node scripts/generate.ts",
    "generate": "ts-node scripts/generate.ts",
    "install-fuses": "node scripts/install-fuses.mjs"
  },
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@electron/packager": "18.1.1",
    "@electron-forge/cli": "7.2.0",
    "@electron-forge/plugin-webpack": "7.2.0",
    "@types/chai": "4.3.11",
    "@types/crc": "3.8.3",
    "@types/line-reader": "0.0.37",
    "@types/lodash.debounce": "4.0.9",
    "@types/mocha": "10.0.6",
    "@types/node": "20.10.4",
    "@types/properties-reader": "2.1.3",
    "@types/sinon": "17.0.2",
    "@types/uuid": "9.0.7",
    "@types/winston-syslog": "2.4.3",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "chai": "4.3.10",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "electron": "28.3.1",
    "electron-mocha": "12.2.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-promise": "6.1.1",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "json": "11.0.0",
    "json-schema-to-typescript": "13.1.1",
    "lint-staged": "15.2.0",
    "mocha": "10.2.0",
    "nan": "2.18.0",
    "node-loader": "2.0.0",
    "nyc": "15.1.0",
    "process": "0.11.10",
    "prettier": "3.1.1",
    "sinon": "17.0.1",
    "style-loader": "3.3.3",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "vue-loader": "17.3.1",
    "vue-template-compiler": "2.7.15",
    "webpack": "5.89.0"
  },
  "dependencies": {
    "@electron/fuses": "^1.7.0",
    "@vueuse/core": "10.7.0",
    "crc": "4.3.2",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "8.1.0",
    "i18next": "23.7.10",
    "line-reader": "0.4.0",
    "lodash.debounce": "4.0.8",
    "net-ipc": "2.1.0",
