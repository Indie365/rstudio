pipeline {
  agent {
    dockerfile {
      filename 'Dockerfile.dispatcher'
      label 'linux && amd64'
    }
  }

  parameters {
    gitParameter
    defaultValue: "${env.BRANCH_NAME}",
    name: 'COMMIT_HASH',
    type: 'PT_REVISION'
  }


  stages {
    stage ("Define Variables") {
      steps {
        echo "Commit_hash value: ${params.COMMIT_HASH}"
      }
    }
  }
}